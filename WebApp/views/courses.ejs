<% include header %>
<% include navBar %>

<div class = "container">
	<div class = "row">
  <% if (courses.length) { %>
    <% courses.forEach(function(entry) { %>
    <div class="col-sm-4">
      <div class="panel-group">
        <div class="panel panel-info">
          <div class="panel-heading"><a href = "\courses\<%= entry.CRN %>\<%= entry.count %>"><%= entry.CRN %></a></div>
          <div class="panel-body"><img src="pics\<%= entry.title %>.jpeg" style="width:100%;height:9em;"></div>
          <div class="panel-footer"><%= entry.title %></a></div>
        </div>
      </div>
    </div>
    <% }) %>
  <% } else { %>
    <div class = "container">
      <div  class = "jumbotron">
          <h1> Add some courses </h2>
      </div>
    </div>
  <%} %>	
		
	</div>
</div>

<% include footer %>